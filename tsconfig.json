/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "skipLibCheck": true,

    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "resolveJsonModule": true,
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "auth": [
        "dist/auth/auth",
        "dist/auth"
      ],
      "markdown": [
        "dist/markdown/markdown",
        "dist/markdown"
      ],
      "storage": [
        "dist/storage/storage",
        "dist/storage"
      ]
    },
    "allowSyntheticDefaultImports": true
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "enableIvy": false
  }
}
